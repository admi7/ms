<!DOCTYPE>
<html lang="fr">
	<head>
		{% block meta %}
		<meta charset="utf-8">
	{% endblock %}
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			<link rel="stylesheet" href="node_modules/@fortawesome/fontawesome-free/css/all.css">
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{# <script src="https://kit.fontawesome.com/7c56b612cb.js" crossorigin="anonymous" defer></script> #}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body class="fce white">
		<div id="app">
			{% block nav %}{% endblock %}
			{% block body %}{% endblock %}
			{% block footer %}
				{% include "resource/_footer.html.twig" %}
			{% endblock %}

		</div>
		<div id="add"></div>
	</body>
</html>
